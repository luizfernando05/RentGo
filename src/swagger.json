    "/orders": {
      "post": {
        "summary": "Cria um novo pedido",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "tags": ["Orders"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "clientId": {
                    "type": "string",
                    "description": "ID do cliente."
